# 环境配置
NODE_ENV=development
PORT=3000

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=root
DB_NAME=photograph_app

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT配置
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=2h
JWT_REFRESH_EXPIRES_IN=7d

# 文件上传配置
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760

# 验证码配置
SMS_APP_ID=your-app-id
SMS_APP_KEY=your-app-key

# 千问AI配置
# 申请地址: https://dashscope.aliyun.com/
QWEN_API_KEY=your-qwen-api-key
